<a class="show-return"
   href="#shows/<%= escape(episode.get('fShowTitle'))%>"
  >Return to: <%= episode.escape('fShowTitle').replace(/_/g, ' ')%></a>


<article class="page-basics">
  <h1><%= episode.escape('title') %></h1>
  <span>S<%= episode.escape('season') %>E<%= episode.escape('position') %></span>
  <section class="page-info group">
    <p>
      Description:
      <%= episode.escape('description') %>
    </p>

    <div class="episode-pic">
      <img src="<%=episode.escape('image_url') %>"/>
    </div>
  </section>
</article>

<ul class="post-list"></ul>

<div class="post-form"></div>
